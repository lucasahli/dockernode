{
  "name": "dockernode",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "start": "node dist/index.js",
    "dev": "concurrently \"pnpm run watch-compile\" \"pnpm run watch-dev\"",
    "watch-compile": "swc ./src -w -d dist",
    "watch-dev": "nodemon -L --watch \"dist/**/*\" -e js ./dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.75",
    "@tsconfig/node18": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.8",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1"
  }
}

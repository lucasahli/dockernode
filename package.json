{
  "name": "dockernode",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "start": "node dist/main.js",
    "dev": "concurrently \"pnpm run watch-compile\" \"pnpm run watch-dev\"",
    "watch-compile": "swc ./src -w -d dist",
    "watch-dev": "nodemon -L --inspect=0.0.0.0:9229 --watch \"dist/**/*\" -e js ./dist/main.js",
    "tests": "jest --config=jest.config.cjs",
    "testing": "node --trace-warnings --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.config.cjs --no-cache"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "node --trace-uncaught"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.7.1",
    "graphql-config": "^5.0.2",
    "graphql-http": "^1.21.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "pnpm": "^8.7.0",
    "redis": "^4.6.7",
    "sequelize": "^6.32.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.75",
    "@swc/jest": "^0.2.29",
    "@tsconfig/node18": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.8",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}

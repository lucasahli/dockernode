{
  "name": "dockernode",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "start": "node dist/main.js",
    "dev": "concurrently \"pnpm run watch-compile\" \"pnpm run watch-dev\"",
    "watch-compile": "swc ./src -w -d dist",
    "watch-dev": "nodemon -L --watch \"dist/**/*\" -e js ./dist/main.js"
  },
